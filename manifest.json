{
    "manifest_version": 3,
    "name": "Twitch-Extension",
    "version": "1.0",
    "description": "This is an extension that provides a variety of twitch functions.",

    "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.twitch.tv/*"],
            "js": ["main.js"]
        }
    ]
}