{
    "manifest_version": 3,
    "name": "Twitch-Auto-Reward",
    "version": "1.0",
    "description": "Automatically accept Twitch's reward.",

    "icons": {
        "16":"images/icon.png",
        "48":"images/icon.png",
        "64":"images/icon.png",
        "128":"images/icon.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.twitch.tv/*"],
            "js": ["main.js"]
        }
    ]
}